{
  "name": "digitalpanel",
  "displayName": "Digitalpanel Downloader",
  "version": "3.6",
  "description": "Download resources from microstock via Digital Panel",
  "author": "byme@digitalpanel.id",
  "scripts": {
    "dev": "plasmo dev",
    "build": "cross-env PARCEL_WORKER_BACKEND=process plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "plasmo": "0.90.3",
    "pusher-js": "^8.4.0-rc2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.31",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.1",
    "prettier": "3.2.4",
    "tailwindcss": "^3.3.0",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "*://*.freepik.com/*",
      "*://*.motionarray.com/*",
      "*://*.elements.envato.com/*",
      "https://app.digitalpanel.id/*"
    ],
    "permissions": [
      "tabs",
      "storage",
      "notifications",
      "downloads",
      "sidePanel",
      "cookies",
      "webRequest"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "assets/*"
        ],
        "matches": [
          "*://*.freepik.com/*",
          "*://*.motionarray.com/*",
          "*://*.elements.envato.com/*",
          "https://app.digitalpanel.id/*"
        ]
      }
    ],
    "side_panel": {
      "default_path": "sidepanel.html"
    },
    "action": {
      "default_title": "DigitalPanel",
      "default_icon": {
        "16": "/assets/icon16.png",
        "48": "/assets/icon48.png",
        "128": "/assets/icon128.png"
      }
    },
    "icons": {
      "16": "/assets/icon16.png",
      "48": "/assets/icon48.png",
      "128": "/assets/icon128.png"
    }
  },
  "entrypoints": {
    "sidepanel": "src/sidepanel.tsx"
  }
}
